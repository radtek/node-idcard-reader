{
  "name": "idcard-reader",
  "version": "2.0.2",
  "description": "通过二代身份证机具读取二代身份证信息",
  "main": "./dist/index.js",
  "bin": {
    "idc-reader": "./dist/bin/idc-reader.js"
  },
  "scripts": {
    "build": "npm run clean && npm run lint && npm run build:ts && npm run jslint",
    "build:ts": "tsc -p ./ --outDir dist/",
    "clean": "rm -rf dist/*",
    "cov": "nyc mocha",
    "cov:coveralls": "nyc mocha && nyc report --reporter=text-lcov | coveralls",
    "jslint": "eslint --fix dist/**/*.js",
    "idc-reader": "node ./dist/bin/idc-reader.js",
    "lint": "tslint --fix -p tsconfig.json -e **/*.d.ts -t stylish src/**/*.ts test/**/*.ts",
    "prepublishOnly": "npm run build",
    "test": "npm run test:appveyor",
    "test:appveyor": "mocha --config test/mocha.appveyor.opts",
    "test:travis": "mocha --config test/mocha.travis.opts"
  },
  "keywords": [
    "idcard",
    "idc-reader",
    "身份证",
    "二代身份证",
    "二代证机具"
  ],
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "author": "waiting",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/waitingsong/node-idcard-reader.git"
  },
  "bugs": {
    "url": "https://github.com/waitingsong/node-idcard-reader/issues"
  },
  "homepage": "https://github.com/waitingsong/node-idcard-reader#readme",
  "engines": {
    "node": ">=8.9.0"
  },
  "nyc": {
    "exclude": [
      "**/*.js",
      "**/*.test.ts",
      "**/*.d.ts",
      "test",
      "dist",
      "src/bin"
    ],
    "extension": [
      ".ts",
      ".tsx"
    ],
    "reporter": [
      "text-summary",
      "html"
    ],
    "all": true
  },
  "dependencies": {
    "ffi": "^2.2.0"
  },
  "devDependencies": {
    "@types/ffi": "^0.1.0",
    "@types/mocha": "^2.2.48",
    "@types/node": "^8.9.5",
    "@types/power-assert": "^1.4.29",
    "@types/rewire": "^2.5.28",
    "@types/rimraf": "^2.0.2",
    "coveralls": "^3.0.0",
    "eslint": "^4.18.2",
    "intelli-espower-loader": "^1.0.1",
    "istanbul": "^0.4.5",
    "mocha": "^5.0.3",
    "mocha-lcov-reporter": "^1.3.0",
    "mz-modules": "^2.1.0",
    "nyc": "^11.5.0",
    "power-assert": "^1.4.4",
    "rewire": "^3.0.2",
    "rimraf": "^2.6.2",
    "source-map-support": "^0.5.3",
    "ts-node": "^5.0.1",
    "tslint": "^5.9.1",
    "tslint-eslint-rules": "^5.1.0",
    "typescript": "^2.7.2"
  }
}
